<Pane prefHeight="902.0" prefWidth="1021.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mike.personalfitnesstracker.SignUpController">
   <children>
      <AnchorPane prefHeight="902.0" prefWidth="1021.0">
         <children>
            <Label layoutX="458.0" layoutY="14.0" text="Please Sign Up">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <GridPane layoutX="275.0" layoutY="39.0" prefHeight="589.0" prefWidth="471.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label text="Enter Username">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="Enter Password" GridPane.rowIndex="1">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="enterUserTF" GridPane.columnIndex="1" />
                  <TextField fx:id="enterPasswordTF" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Name" GridPane.rowIndex="3">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="Age" GridPane.rowIndex="4">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="Height (inches)" GridPane.rowIndex="5">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="Current Weight (Ibs.)" GridPane.rowIndex="6">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="enterNameTF" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="enterAgeTF" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <TextField fx:id="enterHeightTF" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <TextField fx:id="enterCurWeightTF" GridPane.columnIndex="1" GridPane.rowIndex="6" />
