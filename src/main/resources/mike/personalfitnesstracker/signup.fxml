            <CheckBox fx:id="gainMuscleCB" layoutX="649.0" layoutY="730.0" mnemonicParsing="false" onAction="#getGoals" text="Gain muscle mass" />
            <CheckBox fx:id="RippedCB" layoutX="830.0" layoutY="728.0" mnemonicParsing="false" onAction="#getGoals" text="Get ripped" />
