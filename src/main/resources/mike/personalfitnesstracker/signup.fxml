                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="enterTargetWeightTF" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                  <Label text="Enter Email" GridPane.rowIndex="2">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="enterEmailTF" GridPane.columnIndex="1" GridPane.rowIndex="2" />
